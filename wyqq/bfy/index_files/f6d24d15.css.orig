.xg-right-bar {
 align-items:center;
 display:flex;
 justify-content:center
}
.xgplayer-mini .xg-right-bar {
 display:none!important
}
.xgplayer-container-inactive .xgplayer-playswitch .xgplayer-playswitch-tab {
 opacity:.4
}
.xgplayer-playswitch {
 flex-shrink:0
}
.xgplayer-playswitch svg path {
 fill:#fff
}
.xgplayer-playswitch .xgplayer-playswitch-tab {
 background-color:#323442;
 border-radius:18px;
 height:80px;
 opacity:.7;
 position:relative;
 width:36px
}
.xgplayer-playswitch .xgplayer-playswitch-tab:hover {
 opacity:.9
}
.xgplayer-playswitch .xgplayer-playswitch-prev {
 height:40px;
 opacity:.7;
 position:absolute;
 top:0;
 width:36px
}
.xgplayer-playswitch .xgplayer-playswitch-next {
 bottom:0;
 height:40px;
 opacity:.7;
 position:absolute;
 width:36px
}
.xgplayer-playswitch .xgplayer-playswitch-next.disabled,
.xgplayer-playswitch .xgplayer-playswitch-prev.disabled {
 cursor:not-allowed;
 opacity:.3
}
.xgplayer-playswitch .xgplayer-playswitch-next:not(.disabled):hover,
.xgplayer-playswitch .xgplayer-playswitch-prev:not(.disabled):hover {
 cursor:pointer;
 opacity:1
}
.xgplayer-playswitch svg {
 height:26px!important;
 left:5px;
 position:relative;
 top:7px;
 width:26px!important
}
